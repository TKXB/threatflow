          "action": { "enum": ["add", "assign"], "default": "add" },
          "trustZone": {
            "type": "object",
            "properties": { "id": { "type": "string" }, "name": { "type": "string" } }
          },
          "componentId": { "type": "string" },
          "trustZoneId": { "type": "string" }
        },
        "required": ["baseUrl", "action"]
      }
