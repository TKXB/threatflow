{
  "name": "threatflow-ui-extensions",
  "version": "0.1.0",
  "nodes": [
    {
      "id": "DataflowEditor",
      "name": "Dataflow Editor",
      "category": "OTM",
      "inputs": ["otm"],
      "outputs": ["otm"],
      "configSchema": {
        "type": "object",
        "properties": {
          "baseUrl": { "type": "string", "default": "http://127.0.0.1:8889" },
          "action": { "enum": ["add", "remove"], "default": "add" },
          "dataflow": {
            "type": "object",
            "properties": {
              "id": { "type": "string" },
              "source": { "type": "string" },
              "destination": { "type": "string" },
              "protocol": { "type": "string" }
            }
          },
          "id": { "type": "string" }
        },
        "required": ["baseUrl", "action"]
      }
    },
    {
